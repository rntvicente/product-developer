{
  "check-coverage": true,
  "extension": [".js"],
  "exclude": [
    "test",
    "src/auth/*.js",
    "src/database/*.js",
    "src/error-handlers/*.js",
    "src/server/*.js"
  ],
  "statements": 50,
  "branches": 50,
  "functions": 50,
  "lines": 50,
  "reporter": ["lcov", "html", "text", "text-summary"],
  "watermarks": {
    "lines": [30, 99],
    "functions": [30, 99],
    "branches": [30, 99],
    "statements": [30, 99]
  }
}
